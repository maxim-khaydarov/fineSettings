<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingPrefix"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:toggle="http://schemas.android.com/apk/res-auto"
    android:background="@color/background_fon"
    android:ems="10">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_fon"
        android:orientation="horizontal"></LinearLayout>

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/scrollView1"
        style="android:scrollViewStyle"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/background_fon"
        android:ems="10"
        android:fadingEdge="none"
        android:overScrollMode="always"
        android:scrollbars="vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:ems="10"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ImageView38"
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:layout_marginTop="80dp"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <LinearLayout
                android:id="@+id/LinearLayoutSotSvyaz"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:background="@color/white"
                android:ems="10"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/ButtonAccount"
                        style="?textTitle"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_centerVertical="true"
                        android:background="@android:color/transparent"
                        android:ems="10"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="@string/account_mail"
                        android:textColor="@color/black" />

                    <ImageView
                        android:id="@+id/ImageView05"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/next" />

                </RelativeLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/ImageView03"
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <TextView
                android:id="@+id/textView46"
                style="?textDop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
                android:text="@string/messages"
                android:textAllCaps="true"
                android:textColor="@color/hint_text" />

            <ImageView
                android:id="@+id/imageView32"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <LinearLayout
                android:id="@+id/LinearLayoutAir1"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:background="@color/white"
                android:ems="10"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:ems="10">

                    <ImageView
                        android:id="@+id/imageView45"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/next" />

                    <Button
                        android:id="@+id/Button011"
                        style="?textTitle"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/button_bg"
                        android:clickable="false"
                        android:ems="10"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="@string/sort_calendar"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/textView50"
                        style="?textTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="15dp"
                        android:layout_toLeftOf="@+id/imageView45"
                        android:gravity="center_vertical"
                        android:text=""
                        android:textColor="@color/hint_text" />

                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout141"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:background="@color/white"
                android:ems="10"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:ems="10">

                    <ImageView
                        android:id="@+id/imageView46"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/next" />

                    <Button
                        android:id="@+id/Button151"
                        style="?textTitle"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/button_bg"
                        android:clickable="false"
                        android:ems="10"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="@string/otobrazhenie_calendar"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/textView51"
                        style="?textTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="15dp"
                        android:layout_toLeftOf="@+id/imageView46"
                        android:gravity="center_vertical"
                        android:textColor="@color/hint_text" />

                    <ImageView
                        android:id="@+id/ImageView181"
                        android:layout_width="fill_parent"
                        android:layout_height="1px"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:ems="10"
                        android:scaleType="fitXY"
                        android:src="@drawable/line_cray" />
                </RelativeLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/imageView37"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="15dp"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <LinearLayout
                android:id="@+id/LinearLayoutAir111"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:background="@color/white"
                android:ems="10"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:ems="10">

                    <Button
                        android:id="@+id/Button0111"
                        style="?textTitle"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/button_bg"
                        android:clickable="false"
                        android:ems="10"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="@string/apps_contact"
                        android:textColor="@color/black" />

                    <com.suke.widget.SwitchButton
                        android:id="@+id/tg_1"
                        android:layout_width="55dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        toggle:sb_show_indicator="false" />
                </RelativeLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/imageView38"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <TextView
                android:id="@+id/textView48"
                style="?textDop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="8dp"
                android:text="@string/apps_text_contact"
                android:textColor="@color/hint_text" />

        </LinearLayout>

    </ScrollView>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@color/background_title"
        android:orientation="vertical" >

        <include
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            layout="@layout/title_status" />
    </LinearLayout>

</RelativeLayout>